{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es6",
    "lib": ["esnext"],
    "esModuleInterop": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "allowJs": false,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "incremental": true,
    "types": ["node"]
  },
  "exclude": ["cdk.out"]
}
